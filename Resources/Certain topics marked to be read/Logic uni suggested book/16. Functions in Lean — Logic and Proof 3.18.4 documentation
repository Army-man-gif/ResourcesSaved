    <span class="n">cases</span> <span class="n">h</span> <span class="k">with</span>
    <span class="bp">|</span> <span class="n">intro</span> <span class="n">x</span> <span class="n">hx</span> <span class="bp">=&gt;</span>
      <span class="k">have</span> <span class="n">xA₁A₂</span> <span class="o">:</span> <span class="n">x</span> <span class="bp">∈</span> <span class="n">A₁</span> <span class="bp">∪</span> <span class="n">A₂</span> <span class="o">:=</span> <span class="n">hx.left</span>
      <span class="k">have</span> <span class="n">fxy</span> <span class="o">:</span> <span class="n">f</span> <span class="n">x</span> <span class="bp">=</span> <span class="n">y</span> <span class="o">:=</span> <span class="n">hx.right</span>
      <span class="n">cases</span> <span class="n">xA₁A₂</span> <span class="k">with</span>
      <span class="bp">|</span> <span class="n">inl</span> <span class="n">xA₁</span> <span class="bp">=&gt;</span> <span class="n">exact</span> <span class="n">Or.inl</span> <span class="o">⟨</span><span class="n">x</span><span class="o">,</span> <span class="n">xA₁</span><span class="o">,</span> <span class="n">fxy</span><span class="o">⟩</span>
      <span class="bp">|</span> <span class="n">inr</span> <span class="n">xA₂</span> <span class="bp">=&gt;</span> <span class="n">exact</span> <span class="n">Or.inr</span> <span class="o">⟨</span><span class="n">x</span><span class="o">,</span> <span class="n">xA₂</span><span class="o">,</span> <span class="n">fxy</span><span class="o">⟩</span>
  <span class="bp">.</span> <span class="n">intro</span> <span class="o">(</span><span class="n">h</span> <span class="o">:</span> <span class="n">y</span> <span class="bp">∈</span> <span class="n">image</span> <span class="n">f</span> <span class="n">A₁</span> <span class="bp">∪</span> <span class="n">image</span> <span class="n">f</span> <span class="n">A₂</span><span class="o">)</span>
    <span class="n">cases</span> <span class="n">h</span> <span class="k">with</span>
    <span class="bp">|</span> <span class="n">inl</span> <span class="n">yifA₁</span> <span class="bp">=&gt;</span>
      <span class="n">cases</span> <span class="n">yifA₁</span> <span class="k">with</span>
